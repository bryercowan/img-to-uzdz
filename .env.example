# Database (for local Docker development)
DATABASE_URL=postgresql://imgto3d:password123@postgres:5432/imgto3d

# Redis (for job queue and sessions)
REDIS_URL=redis://redis:6379/0

# JWT Authentication
SECRET_KEY=your-super-secret-key-change-in-production

# Application URLs
BASE_URL=http://localhost:3000

# Stripe Payment Processing (required for Studio tier)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PRICE_ID=price_your_stripe_price_id_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Google Cloud Vision API (for image analysis) - REQUIRED
GOOGLE_CLOUD_PROJECT_ID=your-gcp-project-id
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account-key.json

# AWS S3 Storage (for file uploads)
S3_ENDPOINT=
S3_ACCESS_KEY=your_s3_access_key
S3_SECRET_KEY=your_s3_secret_key
S3_BUCKET=imgto3d-storage
S3_REGION=us-east-1

# RunPod GPU Processing (for 3D generation)
RUNPOD_API_KEY=your-runpod-api-key
RUNPOD_ENDPOINT_ID=your-runpod-endpoint-id

# Lambda Labs GPU Processing (deprecated - use RunPod instead)
LAMBDA_LABS_API_KEY=your-lambda-labs-api-key
EXTERNAL_REDIS_URL=redis://your-public-redis-url:6379

# Optional: Job Configuration (has defaults)
JOB_MAX_ITER_FAST=3000
JOB_MAX_ITER_HIGH=8000
RUSH_QUEUE_ENABLED=false

# Optional: Credit System (has defaults)
CREDITS_FAST_JOB=1.0
CREDITS_HIGH_JOB=2.5
CREDITS_RUSH_MULTIPLIER=1.5

# Optional: Features
FEATURE_USDZ=false